# Don't need the executables
--disable-ffmpeg
--disable-ffserver
--disable-ffplay

# Keep size down
--disable-debug
--disable-encoders
--disable-muxers
--disable-protocols
--enable-protocol=file
--disable-devices

# These are usually disabled, but make sure
--disable-network
--disable-ipv6
--disable-zlib
--disable-vhook
